s_bStaCenteringIdle := s_cmCenteringUnit.p_bStaIsOff;

s_diStaAtInfeedPositionRear();
s_diStaAtInfeedPositionFront();
s_diStaAtCenteringPosition();
s_diStaAtDockingPosition();
