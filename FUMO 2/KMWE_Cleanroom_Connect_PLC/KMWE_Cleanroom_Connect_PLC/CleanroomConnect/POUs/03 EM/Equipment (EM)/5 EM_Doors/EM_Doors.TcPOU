	s_bReqCloseOuterSpeedDoor : BOOL := FALSE;
	s_bReqOpenInnerSpeedDoor : BOOL := FALSE;
	s_bReqCloseInnerSpeedDoor : BOOL := FALSE;
//++	s_bReqReleaseCleanroomDoor : BOOL := FALSE;
