	s_fbAlarmHandlerOuterSpeedDoorNoAllowedToClose : FB_AlarmHandler;
	s_fbAlarmHandlerInnerSpeedDoorNoAllowedToOpen : FB_AlarmHandler;
	s_fbAlarmHandlerInnerSpeedDoorNoAllowedToClose : FB_AlarmHandler;
