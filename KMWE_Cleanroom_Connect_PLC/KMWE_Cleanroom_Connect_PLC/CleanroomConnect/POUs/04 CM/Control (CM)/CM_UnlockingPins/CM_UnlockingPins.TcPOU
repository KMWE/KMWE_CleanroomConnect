    <Method Name="_m_Init" Id="{93ef7758-cb22-4383-acb2-e5491d520152}" FolderPath="Methods\">
      <Declaration><![CDATA[METHOD PROTECTED _m_Init : BOOL
]]></Declaration>
      <Implementation>
        <ST><![CDATA[
IF NOT SUPER^._m_Init() THEN
	RETURN;
END_IF

s_cmValveUnlockingPinsUpper.i_stSettings.tValveOffTimeout := i_stSettings.tPinsLockTimeout;
s_cmValveUnlockingPinsUpper.i_stSettings.tValveOnTimeout := i_stSettings.tPinsUnlockTimeout;

s_cmValveUnlockingPinsLower.i_stSettings.tValveOffTimeout := i_stSettings.tPinsLockTimeout;
s_cmValveUnlockingPinsLower.i_stSettings.tValveOnTimeout := i_stSettings.tPinsUnlockTimeout;

_m_Init := TRUE;
]]></ST>
      </Implementation>
    </Method>
