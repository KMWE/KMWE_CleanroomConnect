  <POU Name="CM_UnlockingPins" Id="{5fe56a10-b71c-419b-b351-f5f486dbde09}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION_BLOCK CM_UnlockingPins EXTENDS CM_Base

VAR_INPUT
	i_stSettings : ST_SettingsUnlockingPins;
END_VAR

VAR
	// Control Modules
	s_cmValveUnlockingPinsUpper : CM_ValveWithFB;
	s_cmValveUnlockingPinsLower : CM_ValveWithFB;

	// Request
	s_bReqLock : BOOL;
	s_bReqUnlock : BOOL;

	// Status
	s_bStaIsLocked : BOOL;
	s_bStaIsUnlocked : BOOL; 
	
	{attribute: 'Hide'}
	s_bStaPins : BOOL; //FALSE = Locked, TRUE = Unlocked
	
	// Alarms
	
	// Timers
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[
SUPER^();
]]></ST>
    </Implementation>
    <Folder Name="Methods" Id="{ac78ef67-937f-49b3-9f14-88c938c1bbfb}" />
    <Folder Name="Properties" Id="{8bc233e1-01fd-4210-8ab8-ae64d186d63e}" />
    <Method Name="_m_Alarms" Id="{ea5cad21-9a17-4158-8d08-1b5d16ae26f5}" FolderPath="Methods\">
      <Declaration><![CDATA[METHOD PROTECTED _m_Alarms : BOOL
]]></Declaration>
      <Implementation>
        <ST><![CDATA[
IF NOT SUPER^._m_Alarms() THEN
	RETURN;
END_IF

s_bStaError := 
	s_cmValveUnlockingPinsUpper.p_bStaError
	OR s_cmValveUnlockingPinsLower.p_bStaError;

_m_Alarms := TRUE;
]]></ST>
      </Implementation>
    </Method>
    <Method Name="_m_Devices" Id="{f60a6693-6f78-44cd-a431-f829a283907f}" FolderPath="Methods\">
      <Declaration><![CDATA[METHOD PROTECTED _m_Devices : BOOL
]]></Declaration>
      <Implementation>
        <ST><![CDATA[
IF NOT SUPER^._m_Devices() THEN
	RETURN;
END_IF

//Call lower-layer devices
s_cmValveUnlockingPinsUpper(
	i_bReqEnable:= i_bReqEnable, 
	i_eCtrlMode:= i_eCtrlMode, 
	i_bCmdReset:= i_bCmdReset, 
	i_sCmName:= 'Upper Unlocking Pins');
	
s_cmValveUnlockingPinsLower(
	i_bReqEnable:= i_bReqEnable, 
	i_eCtrlMode:= i_eCtrlMode, 
	i_bCmdReset:= i_bCmdReset, 
	i_sCmName:= 'Lower Unlocking Pins');	

//Control lower-layer devcies
IF s_bReqLock THEN
	s_cmValveUnlockingPinsUpper.m_Off();
	s_cmValveUnlockingPinsLower.m_Off();
ELSIF s_bReqUnlock THEN
	s_cmValveUnlockingPinsUpper.m_On();
	s_cmValveUnlockingPinsLower.m_On();
ELSE
	s_cmValveUnlockingPinsUpper.m_Stop();
	s_cmValveUnlockingPinsLower.m_Stop();
END_IF

//Statusses
s_bStaIsLocked := s_cmValveUnlockingPinsUpper.p_bStaIsOff AND s_cmValveUnlockingPinsLower.p_bStaIsOff;
s_bStaIsUnlocked := s_cmValveUnlockingPinsUpper.p_bStaIsOn AND s_cmValveUnlockingPinsLower.p_bStaIsOn;

s_bStaPins := NOT(s_bStaIsLocked) AND s_bStaIsUnlocked;

_m_Devices := TRUE;
]]></ST>
      </Implementation>
    </Method>
    <Method Name="_m_Hmi" Id="{77b901e0-422e-4a24-8e53-08b881d5ddf3}" FolderPath="Methods\">
